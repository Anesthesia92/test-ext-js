{
  "name": "fashion",
  "version": "1.0.0",
  "description": "Sass implementation in JavaScript",
  "main": "index.js",
  "bin": {
    "fashion": "./cli.js"
  },
  "directories": {
    "test": "test"
  },
  "files": [
